<table>
  <tr>
    <th>User id:</th>
    <th>First Name</th>
    <th>Last name</th>
    <th>Phone number:</th>
    <th>Email:</th>
  </tr>
  <tr>
    <% @user.each do |t| %>
    <tr>
      <td><%= t.id %></td>
      <td><%= t.first_name %></td>
      <td><%= t.last_name %></td>
      <td><%= t.phone %></td>
      <td><%= t.email %></td>
    </tr>
  <% end %>