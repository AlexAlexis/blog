<%= link_to("Back to list", root_path, class: 'btn btn-primary active') %>
<h1>User#new</h1>
<p>Find me in app/views/user/new.html.erb</p>
<%= form_for(@user) do |f| %>
  <p>First name</p>
  <%= f.text_field(:first_name) %>
  <p>Last name</p>
  <%= f.text_field(:last_name) %>
  <p>Phone</p>
  <%= f.text_field(:phone) %>
  <p>Email</p>
  <%= f.text_field(:email) %>
  <%= submit_tag("Enter") %>


<% end %>
