<% @user1.each do |user| %>
  <tr id='<%= user.id %>'>
    <td><%= link_to(user.first_name, user, class: 'link') %></td>
    <td><%= user.last_name %></td>
    <td><%= user.phone %></td>
    <td><%= user.email %></td>
    <td><%= link_to("update", edit_user_path(user), class: 'btn btn-primary btn-lg') %></td>
    <td><%= link_to("delete", delete_user_path(user), class: 'btn btn-primary btn-danger') %></td>
    <td class='destroy'>Destroy</td>
  </tr>

<% end %>